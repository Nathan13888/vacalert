{
  "name": "va",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "del-cli dist",
    "build": "ng build",
    "prod": "npm run clean && ng build --prod",
    "server": "angular-http-server -p 8080 --path dist/va",
    "test": "ng test",
    "docker:build": "docker build -t va-ui .",
    "docker:run": "docker run -p 4200:80 -d -v $(pwd)/nginx.conf:/etc/nginx/nginx.conf:ro --name va-ui va-ui:latest",
    "docker:logs": "docker logs va-ui",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix=true",
    "e2e": "ng e2e",
    "browsers": "(echo var browsers = && browserslist-useragent-regexp --allowHigherVersions) > src/assets/browsers.js",
    "api-clean": "trash src/app/api/**",
    "api-gen": "openapi-generator-cli generate -i ../openapi.json -o src/app/api -g typescript-angular -p ngVersion=11.0.0",
    "apic": "cd ../va-api && npm run getapi && cd ../va-ui && npm run api-clean && npm run api-gen && npm run copy-keys",
    "api": "cd ../va-api && npm run getapi && cd ../va-ui && npm run api-gen && npm run copy-keys",
    "all": "concurrently \"npm start\" \"cd ../va-api && npm run watch\" \"cd ../va-api && npm run start-dev\"",
    "copy-keys": "copyfiles --flat ../va-api/src/authorization/permission-key.ts src/app/api/model",
    "deploy-clean": "trash /www/va/**/*",
    "deploy-copy": "copyfiles -a -u 2 ./dist/va/**/* /www/va/",
    "deploy": "npm run deploy-clean && npm run deploy-copy",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.8",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.8",
    "@angular/compiler": "^11.0.8",
    "@angular/core": "^11.0.8",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.0.8",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "^11.0.8",
    "@angular/platform-browser-dynamic": "^11.0.8",
    "@angular/router": "^11.0.8",
    "@juggle/resize-observer": "^3.2.0",
    "canvas-confetti": "^1.3.2",
    "nanoid": "^3.1.20",
    "ngx-slimscroll": "^11.0.0",
    "rxjs": "^6.6.3",
    "swiper": "^6.4.5",
    "tslib": "^2.1.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular/cli": "^11.0.6",
    "@angular/compiler-cli": "^11.0.8",
    "@angular/language-service": "^11.0.8",
    "@types/canvas-confetti": "^1.3.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.14.20",
    "@types/swiper": "^5.4.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "move-cli": "^1.2.1",
    "path-exists-cli": "^1.0.0",
    "protractor": "~7.0.0",
    "trash-cli": "^3.1.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "^4.0.5"
  }
}
